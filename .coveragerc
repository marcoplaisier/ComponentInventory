# Coverage.py configuration

[run]
source = .
omit =
    tests/*
    .venv/*
    venv/*
    */site-packages/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract

precision = 2
show_missing = True

[html]
directory = htmlcov
